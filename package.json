{
	"name": "typeorm-sample",
	"version": "0.0.1",
	"description": "Awesome project developed with TypeORM.",
	"devDependencies": {
		"@types/express": "^5.0.2",
		"@types/node": "^22.13.10",
		"ts-node": "^10.9.2",
		"typescript": "^5.8.2"
	},
	"dependencies": {
		"express": "^5.1.0",
		"pg": "^8.14.1",
		"reflect-metadata": "^0.2.2",
		"typeorm": "0.3.24"
	},
	"scripts": {
		"start": "ts-node src/index.ts",
		"typeorm": "typeorm-ts-node-commonjs",
		"migration:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert --dataSource src/data-source.ts",
		"migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run --dataSource src/data-source.ts",
		"db:seed": "ts-node src/seeders/seed.ts"
	}
}
